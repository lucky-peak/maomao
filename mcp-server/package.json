{
  "name": "@luckypeak-maomao/mcp-server",
  "version": "0.1.0",
  "description": "MCP Server for Maomao Knowledge Base - AI Coding Knowledge Search",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "maomao-mcp": "dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "knowledge-base",
    "vector-search",
    "ollama",
    "qdrant",
    "coding-assistant",
    "rag"
  ],
  "author": "Maomao Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucky-peak/maomao.git",
    "directory": "mcp-server"
  },
  "bugs": {
    "url": "https://github.com/lucky-peak/maomao/issues"
  },
  "homepage": "https://github.com/lucky-peak/maomao#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@qdrant/js-client-rest": "^1.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "vitest": "^3.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
